{
  "name": "aidocs",
  "version": "0.1.0",
  "description": "Generate markdown docs from URLs via CLI.",
  "type": "module",
  "private": false,
  "bin": {
    "aidocs": "./dist/cli.js"
  },
  "main": "./dist/cli.js",
  "exports": {
    ".": "./dist/cli.js",
    "./cli": "./dist/cli.js",
    "./scraper": "./dist/scraper.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build ./src/cli.ts ./src/scraper.ts --outdir dist --target node --format esm --sourcemap --external playwright --external playwright-core --external chromium-bidi --external electron",
    "dev": "bun run src/cli.ts",
    "test": "bun test",
    "lint": "bun x ultracite check",
    "format": "bun x ultracite fix",
    "prepare": "bun run build"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=3.5.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/bun": "latest",
    "ultracite": "7.0.11"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@mozilla/readability": "^0.6.0",
    "@types/jsdom": "^27.0.0",
    "@types/turndown": "^5.0.6",
    "jsdom": "^27.4.0",
    "playwright": "^1.57.0",
    "turndown": "^7.2.2"
  }
}
